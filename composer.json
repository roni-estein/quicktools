{
    "name": "roniestein/quicktools",
    "description": "A set of functions to bootstrap a laravel project with commands enhanced for production speed.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roni Estein",
            "email": "roni.estein@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Quicktools\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.1-dev"
        },
        "laravel": {
            "providers": [
                "Quicktools\\QuicktoolsServiceProvider"
            ],
            "aliases": {
                "Quicktools": "Quicktools\\Facade"
            }
        }
    }
}